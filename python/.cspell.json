        "contentvector",
        "contoso",
        "datamodel",
        "desync",
        "dotenv",
        "endregion",
        "entra",
        "faiss",
        "finalizer",
        "finalizers",
        "genai",
        "generativeai",
        "hnsw",
        "httpx",
        "huggingface",
        "Instrumentor",
        "logit",
        "logprobs",
        "lowlevel",
